{
  "source": {
    "include": ["src/components"], 
    "includePattern": "\\.(vue|js)$"
  },
  "plugins": ["node_modules/jsdoc-vuejs/index.js"],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/minami"
  },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    }
}